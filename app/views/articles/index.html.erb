<h1>List all articles</h1>


<table>
	<tr>
		<th>Article Title</th>
		<th>Article Description</th>
	</tr>
	<% @articles.each do |x| %>
	<tr>
		<td><%= x.title %></td>
		<td><%= x.description %></td>
		<td><%= link_to "Show", article_path(x) %></td>
		<td><%= link_to "Edit", edit_article_path(x) %></td>
	</tr>
	<% end %>
</table>


